int data[8] : [0,1,0,1,2,3,1,1]
float c1[4]:[0.375, 0.375, 0.125, 0.125]
float c2[4]:[0.25, 0.25, 0.25, 0.25]
int hypothesis
int b
b = bernoulli(0.5)
if(b) {
    hypothesis = 0
if(!b){
    hypothesis = 1
}
for (i in 0:8) {
float r
if (hypothesis){
    r = categorical(c1)
}
if (!hypothesis){
    r = categorical(c2)
}
@observe
data[i] == r
}
posterior(hypothesis)
